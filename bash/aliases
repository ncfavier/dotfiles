alias -- -u=--user
alias C='LC_ALL=C'
alias comm='comm --output-delimiter=$'\''\t\t'\'
alias cxa='clip | xargs'
alias cxan='clip | xargs -d\\n'
alias df='df -h'
alias dots='make -C ~/.dots --no-print-directory'
alias dp='declare -p'
alias du='du -h'
alias exec='exec '
alias export='export '
alias fc-grep='fc-list | grep -i'
alias free='LC_ALL=C free -h'
alias ip='ip --color=auto'
alias j='jobs'
alias l='ls'
alias ls='ls --color=auto --group-directories-first'
alias la='ls -lah'
alias ll='ls -lh'
alias lsblk='lsblk -o NAME,TYPE,FSTYPE,LABEL,UUID,SIZE,MOUNTPOINT'
alias mosh='LANG=en_US.UTF-8 MOSH_TITLE_NOPREFIX=y mosh'
alias mv='mv -i'
alias o='xdg-open'
alias ocaml='rlwrap ocaml'
alias rlwrap='rlwrap '
alias sbcl='rlwrap sbcl'
alias sd='systemctl '
alias sda='systemctl start '
alias sde='systemctl enable '
alias sdd='systemctl disable '
alias sded='systemctl edit '
alias sdc='systemctl cat '
alias sdluf='systemctl list-units --failed '
alias sdo='systemctl stop '
alias sdr='systemctl restart '
alias sdrl='systemctl reload '
alias sds='systemctl status '
alias sdu='systemctl --user '
alias sudo='sudo '
alias tall='tail -f -n +1'
alias watch='watch '
alias wg0='wg-quick@wg0'
alias xlog='>&"$XLOGFD" 2>&1 '
alias ffmpeg='ffmpeg -hide_banner'
alias ffprobe='ffprobe -hide_banner'
