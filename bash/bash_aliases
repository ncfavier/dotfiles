alias C='LC_ALL=C'
alias comm='comm --output-delimiter=$'\''\t\t'\'
alias cxa='clip | xargs'
alias df='df -h'
alias dots='make -C ~/git/dotfiles'
alias du='du -h'
alias free='free -h'
alias grep='grep --color=auto'
alias l='ls -lh'
alias ldd='LD_TRACE_LOADED_OBJECTS=1 '
alias ll='ls -lah'
alias ls='ls --color=auto --group-directories-first'
alias lsblk='lsblk -o NAME,TYPE,FSTYPE,LABEL,UUID,SIZE,MOUNTPOINT'
alias o='xdg-open'
alias sudo='sudo '
alias tall='tail -f -n +1'
alias watch='watch '
